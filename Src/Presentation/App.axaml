<Application x:Class="SourceServerManager.Presentation.App"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:e="https://github.com/mrphil2105/MvvmElegance"
             xmlns:local="using:SourceServerManager.Presentation">
    <Application.Resources>
        <e:ApplicationLoader>
            <e:ApplicationLoader.Bootstrapper>
                <local:Bootstrapper />
            </e:ApplicationLoader.Bootstrapper>
        </e:ApplicationLoader>
    </Application.Resources>

    <Application.Styles>
        <FluentTheme Mode="Dark" />
    </Application.Styles>
</Application>
